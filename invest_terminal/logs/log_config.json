{
  "logging": {
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
      "simple": {
        "format": "[%(asctime)s] <%(levelname)s::%(name)s> | (%(filename)s)::%(funcName)s::%(lineno)s -> %(message)s"
      }
    },
    "handlers": {
      "console": {
        "class": "logging.StreamHandler",
        "level": "INFO",
        "formatter": "simple",
        "stream": "ext://sys.stderr"
      }
    },
    "root": {
      "level": "DEBUG",
      "handlers": ["console"]
    }
  }
}